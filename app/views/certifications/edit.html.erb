<% title "Editing #{@certification.name} Teacher Certification", :body => true %>

<% semantic_form_for @certification, :url => certification_url(@certification) do |f| %>
  <%= render 'form', :f => f %>
  <% f.buttons do %>
   <%= f.commit_button 'Submit' %>
   <%= f.cancel_link %>
  <% end %>
<% end %>
